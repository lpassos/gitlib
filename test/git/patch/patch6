-	if ((dev_priv->flags & RADEON_FAMILY_MASK) == CHIP_RS690)       \
-		RS690_WRITE_MCIND(addr, val);				\
-	else								\
-		RS480_WRITE_MCIND(addr, val);				\
-} while (0)
